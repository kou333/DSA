package stack;

public class DynamicStack {
	
	public DynamicStack() {
		
		super();
		// it will call custom stack.
		// it will call cunstructor in parent 
		//calss which does not take any argument.
	    //default constructor.
	      }
		public DynamicStack(int size) 
		{
		   super();	//will call parameterized constructor of parent class
		}
	
		public boolean push(int item)
		{
		
		
			if(this.isFull())
			    {
			
				
				int [] temp=new int[data.length *2];
			
				//copy all previous items in new data
				for(int i=0; i<data.length; i++)
				{
					temp[i]=data[i];
				}
			
				
				 data=temp;
			}
			
			 // here array is not full so 
			return super.push();
			
		}
			
		}


